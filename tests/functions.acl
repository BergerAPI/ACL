import "std"

# adsf
func factorial(num) {
    if num == 0 {
        return 1
    } else {
        return num * factorial(num - 1)
    }
}

println(factorial(10))

func lost(a, b){
    if (a == "los") {
        println(a + " - lost")
    } else if (a == "lost") {
        println(a + " - lost")
    } else {
        println(b)
    }
}

lost("lost", 5)