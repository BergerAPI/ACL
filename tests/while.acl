import "std"

println("starting while")

let i = 0
let m = 0
let startTime = time()

while 1 {
    if i > 1000 {
        break
    }

    while 1 {
        if m > 1000 {
            m = 0
            break
        }

        # println("m", m)
        m = m + 1
    }

    # println("i", i)
    i = i + 1
}

let endTime = time()
let timeElapsed = endTime - startTime

println("time elapsed:", timeElapsed)